-- Mys2 Hub - Full Enhanced Script for Steal a Brainrot

--// Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local StarterGui = game:GetService("StarterGui")
local LocalPlayer = Players.LocalPlayer

--// Data
local mythics = {
    ["Frigo Camelo"] = {"1.2K", "300K"},
    ["Orangutini Ananassini"] = {"1.7K", "400K"},
    ["Rhino Toasterino"] = {"2.1K", "450K"},
    ["Bombardiro Crocodilo"] = {"2.5K", "500K"},
    ["Bombombini Gusini"] = {"5K", "1M"},
    ["Cavallo Virtuoso"] = {"7.5K", "2.5M"},
    ["Spioniro Golubiro"] = {"3.5K", "750K"},
    ["Zibra Zubra Zibralini"] = {"6K", "1M"},
    ["Tigrilini Watermelini"] = {"7.5K", "1M"},
}
local brainrots = {
    ["Cocofanto Elefanto"] = {"10K", "5M"},
    ["Girafa Celestre"] = {"20K", "7.5M"},
    ["Gattatino Neonino"] = {"35K", "7.5M"},
    ["Matteo"] = {"50K", "10M"},
    ["Tralalero Tralala"] = {"50K", "10M"},
    ["Espresso Signora"] = {"70K", "25M"},
    ["Odin Din Din Dun"] = {"75K", "15M"},
    ["Statutino Libertino"] = {"75K", "20M"},
    ["Trenostruzzo Turbo 3000"] = {"150K", "25M"},
    ["Ballerino Lololo"] = {"200K", "35M"},
    ["Tigroligre Frutonni"] = {"60K", "15M"},
    ["Orcalero Orcala"] = {"100K", "15M"},
}

--// ESP
local espMythicEnabled, espBrainrotEnabled, espPlayerEnabled = false, false, false

function createTag(model, text, color)
    local bb = Instance.new("BillboardGui", model)
    bb.Adornee = model:FindFirstChild("PrimaryPart") or model:FindFirstChildWhichIsA("BasePart")
    bb.Size = UDim2.new(0, 200, 0, 50)
    bb.AlwaysOnTop = true
    bb.Name = "ESPInfo"

    local label = Instance.new("TextLabel", bb)
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = color
    label.TextScaled = true
    label.Font = Enum.Font.GothamBold
end

RunService.RenderStepped:Connect(function()
    for _, obj in pairs(Workspace:GetChildren()) do
        if obj:IsA("Model") and not obj:FindFirstChild("ESPInfo") then
            if espMythicEnabled and mythics[obj.Name] then
                createTag(obj, obj.Name .. "\n" .. mythics[obj.Name][1] .. "/sec - " .. mythics[obj.Name][2], Color3.fromRGB(0, 255, 255))
            elseif espBrainrotEnabled and brainrots[obj.Name] then
                createTag(obj, obj.Name .. "\n" .. brainrots[obj.Name][1] .. "/sec - " .. brainrots[obj.Name][2], Color3.fromRGB(0, 0, 255))
            end
        end
    end
    if espPlayerEnabled then
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character and not plr.Character:FindFirstChild("ESPPlayer") then
                local h = Instance.new("Highlight", plr.Character)
                h.Name = "ESPPlayer"
                h.FillColor = Color3.fromRGB(255, 255, 0)
                h.OutlineColor = Color3.new(1, 1, 1)
            end
        end
    end
end)

--// Speed
local speedEnabled = false
local function setSpeed(val)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.WalkSpeed = val
    end
end
RunService.Stepped:Connect(function()
    if speedEnabled then setSpeed(32) end
end)

--// Main GUI
local screenGui = Instance.new("ScreenGui", game.CoreGui)
screenGui.Name = "Mys2MainGui"

local function makeFrame()
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 250, 0, 350)
    frame.Position = UDim2.new(0, 20, 0, 100)
    frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    frame.BorderSizePixel = 0
    frame.Active = true
    frame.Draggable = true
    frame.Parent = screenGui

    local layout = Instance.new("UIListLayout", frame)
    layout.Padding = UDim.new(0, 5)
    layout.SortOrder = Enum.SortOrder.LayoutOrder

    return frame
end

local mainFrame = makeFrame()

local function addButton(text, callback)
    local btn = Instance.new("TextButton", mainFrame)
    btn.Size = UDim2.new(1, -10, 0, 30)
    btn.Position = UDim2.new(0, 5, 0, 0)
    btn.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    btn.TextColor3 = Color3.new(1,1,1)
    btn.BorderSizePixel = 0
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 14
    btn.Text = text
    btn.MouseButton1Click:Connect(function()
        callback(btn)
    end)
end

-- Buttons
addButton("Speed Boost", function(btn)
    speedEnabled = not speedEnabled
    btn.Text = "Speed Boost: " .. (speedEnabled and "ON" or "OFF")
end)

addButton("TP Up", function()
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if hrp then hrp.CFrame = hrp.CFrame + Vector3.new(0, 15, 0) end
end)

addButton("TP Down", function()
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if hrp then hrp.CFrame = hrp.CFrame - Vector3.new(0, 15, 0) end
end)

addButton("Mythic ESP", function(btn)
    espMythicEnabled = not espMythicEnabled
    btn.Text = "Mythic ESP: " .. (espMythicEnabled and "ON" or "OFF")
end)

addButton("Brainrot ESP", function(btn)
    espBrainrotEnabled = not espBrainrotEnabled
    btn.Text = "Brainrot ESP: " .. (espBrainrotEnabled and "ON" or "OFF")
end)

addButton("Player ESP", function(btn)
    espPlayerEnabled = not espPlayerEnabled
    btn.Text = "Player ESP: " .. (espPlayerEnabled and "ON" or "OFF")
end)

addButton("Join Discord", function()
    local popup = Instance.new("Frame", screenGui)
    popup.Size = UDim2.new(0, 300, 0, 150)
    popup.Position = UDim2.new(0.5, -150, 0.5, -75)
    popup.BackgroundColor3 = Color3.fromRGB(30,30,30)
    popup.BorderSizePixel = 0

    local label = Instance.new("TextLabel", popup)
    label.Size = UDim2.new(1, 0, 0.6, 0)
    label.Position = UDim2.new(0, 0, 0, 0)
    label.Text = "Discord link copied to clipboard!\nYou can now close this window."
    label.TextColor3 = Color3.new(1,1,1)
    label.TextScaled = true
    label.BackgroundTransparency = 1
    label.Font = Enum.Font.Gotham

    local closeBtn = Instance.new("TextButton", popup)
    closeBtn.Size = UDim2.new(0, 80, 0, 30)
    closeBtn.Position = UDim2.new(1, -85, 1, -35)
    closeBtn.Text = "Close"
    closeBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    closeBtn.TextColor3 = Color3.new(1,1,1)
    closeBtn.Font = Enum.Font.Gotham
    closeBtn.TextSize = 14
    closeBtn.MouseButton1Click:Connect(function() popup:Destroy() end)

    setclipboard("https://discord.gg/brainrots")
end)

--// Fling GUI
local flingGui = Instance.new("ScreenGui", game.CoreGui)
flingGui.Name = "Mys2FlingGui"

local flingFrame = makeFrame()
flingFrame.Parent = flingGui
flingFrame.Position = UDim2.new(0, 280, 0, 100)

local flingBtn = Instance.new("TextButton", flingFrame)
flingBtn.Size = UDim2.new(1, -10, 0, 30)
flingBtn.Position = UDim2.new(0, 5, 0, 0)
flingBtn.BackgroundColor3 = Color3.fromRGB(100, 30, 30)
flingBtn.Text = "Fling to Corner"
flingBtn.TextColor3 = Color3.new(1, 1, 1)
flingBtn.Font = Enum.Font.Gotham
flingBtn.TextSize = 14
flingBtn.MouseButton1Click:Connect(function()
    local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if root then
        root.Velocity = Vector3.new(999, 999, 999)
        root.CFrame = CFrame.new(9999, 999, 9999)
    end
end)
